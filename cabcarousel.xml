<?xml version="1.0" encoding="UTF-8"?>
<theme>
<formatVersion>7</formatVersion>

	<customView name="cabcarousel" inherits="gamecarousel">
<gamecarousel name="gamecarousel">
<zIndex>43</zIndex>	      
          <type>vertical_wheel</type>
          <pos>0.28 0</pos>
          <size>1 1</size>
<logoScale>1.5</logoScale>
<logoSize>0.150 0.260</logoSize>
<color>00000000</color>
<maxLogoCount>30</maxLogoCount>
<logoRotation>8</logoRotation>
<logoRotationOrigin>3 0.2</logoRotationOrigin>
<logoAlignment>center</logoAlignment>   
<imageSource>marquee</imageSource>
</gamecarousel> 

	<image name="cabs" extra="true">
		<visible>true</visible>  
        </image>

	<image name="char4" extra="true">
		<visible>false</visible>  
        </image>

	<image name="menu1frame" extra="true">
		<visible>false</visible>  
        </image>

   <text name="md_name">
		<visible>false</visible>  
    </text>	

	       <helpsystem name="help">
	 <iconColor>FFFFFF</iconColor>
     <fontSize>0.02</fontSize>
     <origin>0.5 0.5</origin>
     <pos>0.288 0.99</pos>
     <fontPath>./_inc/.ttf</fontPath>
     </helpsystem>

   		<image name="md_thumbnail">
			<origin>0.5 0.5</origin>
			<pos>0.63 0.83</pos>
			<maxSize>0.29 0.29</maxSize>
		<storyboard event="activate">
        <animation property="x" from="-0.35" begin="0" duration="500" autoreverse="false" mode="easyInOut"/>
		</storyboard>
			<zIndex>999</zIndex>
		</image>

	<image name="logos" extra="true">
        <pos>-0.05 0.4</pos>
        <maxSize>0.2 0.2</maxSize>
		<path>./default/_inc/logo.png</path>
		<path>./_inc/logos/${system.theme}.png</path>
		<color>ffffff</color>
	    <rotation>-90</rotation>  
	    <zIndex>999</zIndex>
	</image>

  		<image name="md_marquee">
        <pos>0.3 0.125</pos>
        <maxSize>0.175 0.13</maxSize>
			<linearSmooth>true</linearSmooth>
			<storyboard event="activate">
		<animation property="rotation" to="720" begin="1000" autoreverse="false" duration="500"/>
		<animation property="rotation" to="-720" begin="3000" autoreverse="false" duration="500"/>
			</storyboard>
			<zIndex>999</zIndex> 
		</image>
     		
     <video name="md_video">
		<color>ffffff</color>
        <pos>0.285 0.5</pos>
        <origin>0.5 0.5</origin>
        <size>0.385 0.385</size>
        <roundCorners>0.06</roundCorners> 
		<delay>0.1</delay>
		<effect>none</effect>		 
        <showSnapshotNoVideo>false</showSnapshotNoVideo>
        <showSnapshotDelay>false</showSnapshotDelay>
		<zIndex>1</zIndex>
     </video> 

     <image name="md_image">
        <pos>0.298 0.53</pos>
        <origin>0.5 0.5</origin>
        <size>0.44 0.63</size>
         <zIndex>1</zIndex>         
      </image> 
	
	</customView>
</theme>
